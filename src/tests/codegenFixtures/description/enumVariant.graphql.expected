-----------------
INPUT
----------------- 
enum Enum {
  """
  This one is valid
  """
  VALID
  """
  This one is invalid
  """
  INVALID
}

-----------------
OUTPUT
-----------------
import { GraphQLSchema, GraphQLEnumType } from "graphql";
const EnumType: GraphQLEnumType = new GraphQLEnumType({
    name: "Enum",
    values: {
        VALID: {
            description: "This one is valid",
            value: "VALID"
        },
        INVALID: {
            description: "This one is invalid",
            value: "INVALID"
        }
    }
});
const schema = new GraphQLSchema({
    types: [EnumType]
});
export { schema };
