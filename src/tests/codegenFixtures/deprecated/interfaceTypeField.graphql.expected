-----------------
INPUT
----------------- 
interface MyInterface {
  myField: String @deprecated(reason: "Reason")
}

type Query {
  greeting: MyInterface
}

-----------------
OUTPUT
-----------------
import { GraphQLSchema, GraphQLObjectType, GraphQLInterfaceType, GraphQLString } from "graphql";
const MyInterfaceType: GraphQLInterfaceType = new GraphQLInterfaceType({
    name: "MyInterface",
    fields() {
        return {
            myField: {
                deprecationReason: "Reason",
                name: "myField",
                type: GraphQLString
            }
        };
    }
});
const QueryType: GraphQLObjectType = new GraphQLObjectType({
    name: "Query",
    fields() {
        return {
            greeting: {
                name: "greeting",
                type: MyInterfaceType
            }
        };
    }
});
const schema = new GraphQLSchema({
    query: QueryType,
    types: [MyInterfaceType, QueryType]
});
export { schema };
